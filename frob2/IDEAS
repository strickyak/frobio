extern int puts(const char *s);

/*
class Foo {
  public:
    void bar() {
      puts("hello world");
    }
}
*/

inline void Greet() { 
      puts("hello world");
}

int main() {
  Greet();
  asm (
    "   NOP foobar  "
  );
  unsigned int a=100, b=200, c=300;
  for (unsigned i = 0; i < a; i++) Greet();
  for (unsigned i = 0; i < b; i++) Greet();
  for (unsigned i = 0; i < c; i++) Greet();
  return 13;
}
